okns:EventEvent a okns:ClassDefinition ;
    dcterms:title "Event" ;
    skos:definition "An arbitrary classification of a space/time region, by a ␊		cognitive agent. An event may have actively participating agents,␊		passive factors, products, and a location in space/time." ;
    skos:inScheme okns:event ;
    skos:mappingRelation <event:Event> ;
    linkml:class_uri <http://purl.org/NET/c4dm/event.owl#Event> ;
    linkml:definition_uri okns:EventEvent ;
    linkml:slot_usage [ ] .

