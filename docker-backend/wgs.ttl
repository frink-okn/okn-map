okns:WgsPoint a linkml:ClassDefinition ;
    dcterms:title "point" ;
    skos:definition "A point, typically described using a coordinate system relative to Earth, such as WGS84." ;
    skos:inScheme okns:wgs ;
    skos:mappingRelation <wgs:Point> ;
    skos:note """description: Uniquely identified by lat/long/alt. i.e.

spaciallyIntersects(P1, P2) :- lat(P1, LAT), long(P1, LONG), alt(P1, ALT),
  lat(P2, LAT), long(P2, LONG), alt(P2, ALT).

sameThing(P1, P2) :- type(P1, Point), type(P2, Point), spaciallyIntersects(P1, P2).""" ;
    linkml:class_uri <http://www.w3.org/2003/01/geo/wgs84_pos#Point> ;
    linkml:definition_uri okns:WgsPoint ;
    linkml:is_a okns:WgsSpatialThing ;
    linkml:slot_usage [ ] .

okns:wgs_lat_long a linkml:SlotDefinition ;
    dcterms:title "lat/long" ;
    skos:definition "A comma-separated representation of a latitude, longitude coordinate." ;
    skos:editorialNote "No occurrences of this slot in the graph." ;
    skos:inScheme okns:wgs ;
    skos:mappingRelation <http://www.w3.org/2003/01/geo/wgs84_pos#lat_long> ;
    linkml:definition_uri okns:wgs_lat_long ;
    linkml:inlined true ;
    linkml:range okns:Any ;
    linkml:slot_uri <http://www.w3.org/2003/01/geo/wgs84_pos#lat_long> .

okns:wgs_location a linkml:SlotDefinition ;
    dcterms:title "location" ;
    rdfs:subPropertyOf okns:foaf_based_near ;
    skos:definition "The relation between something and the point, ␊ or other geometrical thing in space, where it is.  For example, the realtionship between␊ a radio tower and a Point with a given lat and long.␊ Or a relationship between a park and its outline as a closed arc of points, or a road and␊ its location as a arc (a sequence of points).␊ Clearly in practice there will be limit to the accuracy of any such statement, but one would expect␊ an accuracy appropriate for the size of the object and uses such as mapping ." ;
    skos:editorialNote "No occurrences of this slot in the graph." ;
    skos:inScheme okns:wgs ;
    skos:mappingRelation <http://www.w3.org/2003/01/geo/wgs84_pos#location> ;
    linkml:definition_uri okns:wgs_location ;
    linkml:inlined true ;
    linkml:range okns:WgsSpatialThing ;
    linkml:slot_uri <http://www.w3.org/2003/01/geo/wgs84_pos#location> .

okns:foaf_based_near a linkml:SlotDefinition ;
    dcterms:title "No slot (predicate) name specified -- this slot is noted as a subproperty of another slot in this graph but has not itself been defined." ;
    skos:editorialNote "No occurrences of this slot in the graph." ;
    skos:inScheme okns:wgs ;
    skos:mappingRelation foaf:based_near ;
    linkml:definition_uri okns:foaf_based_near ;
    linkml:inlined true ;
    linkml:range okns:Any ;
    linkml:slot_uri foaf:based_near .

okns:wgs_alt a linkml:SlotDefinition ;
    dcterms:title "altitude" ;
    skos:definition "The WGS84 altitude of a SpatialThing (decimal meters ␊above the local reference ellipsoid)." ;
    skos:editorialNote "No occurrences of this slot in the graph." ;
    skos:inScheme okns:wgs ;
    skos:mappingRelation <http://www.w3.org/2003/01/geo/wgs84_pos#alt> ;
    linkml:definition_uri okns:wgs_alt ;
    linkml:domain okns:WgsSpatialThing ;
    linkml:inlined true ;
    linkml:owner okns:wgs_alt ;
    linkml:range okns:Any ;
    linkml:slot_uri <http://www.w3.org/2003/01/geo/wgs84_pos#alt> .

okns:wgs_lat a linkml:SlotDefinition ;
    dcterms:title "latitude" ;
    skos:definition "The WGS84 latitude of a SpatialThing (decimal degrees)." ;
    skos:editorialNote "No occurrences of this slot in the graph." ;
    skos:inScheme okns:wgs ;
    skos:mappingRelation <http://www.w3.org/2003/01/geo/wgs84_pos#lat> ;
    linkml:definition_uri okns:wgs_lat ;
