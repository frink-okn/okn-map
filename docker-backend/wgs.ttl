    linkml:inlined true ;
    linkml:owner okns:wgs_lat ;
    linkml:range okns:Any ;
    linkml:slot_uri <http://www.w3.org/2003/01/geo/wgs84_pos#lat> .

okns:wgs_long a linkml:SlotDefinition ;
    dcterms:title "longitude" ;
    skos:definition "The WGS84 longitude of a SpatialThing (decimal degrees)." ;
    skos:editorialNote "No occurrences of this slot in the graph." ;
    skos:inScheme okns:wgs ;
    skos:mappingRelation <http://www.w3.org/2003/01/geo/wgs84_pos#long> ;
    linkml:definition_uri okns:wgs_long ;
    linkml:domain okns:WgsSpatialThing ;
    linkml:inlined true ;
    linkml:owner okns:wgs_long ;
    linkml:range okns:Any ;
    linkml:slot_uri <http://www.w3.org/2003/01/geo/wgs84_pos#long> .

okns:WgsSpatialThing a linkml:ClassDefinition ;
    dcterms:title "SpatialThing" ;
    skos:definition "Anything with spatial extent, i.e. size, shape, or position.␊ e.g. people, places, bowling balls, as well as abstract areas like cubes." ;
    skos:inScheme okns:wgs ;
    skos:mappingRelation <wgs:SpatialThing> ;
    linkml:class_uri <http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing> ;
    linkml:definition_uri okns:WgsSpatialThing ;
    linkml:slot_usage [ ] .

