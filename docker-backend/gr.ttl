okns:GrLicense a okns:ClassDefinition ;
    dcterms:source <http://purl.org/goodrelations/v1> ;
    dcterms:title "License" ;
    skos:definition "A license is the specification of a bundle of rights that determines the type of activity or access offered by the gr:BusinessEntity on the gr:ProductOrService through the gr:Offering.␊	␊Licenses can be standardized (e.g. LPGL, Creative Commons, ...), vendor-specific, or individually defined for a single offer or product. Whether there is a fee for obtaining the license is specified using the gr:UnitPriceSpecification attached to the gr:Offering. Use foaf:page for linking to a document containing the license, e.g. in PDF or HTML." ;
    skos:inScheme okns:gr ;
    skos:mappingRelation ns1:License ;
    linkml:class_uri <http://purl.org/goodrelations/v1#License> ;
    linkml:definition_uri okns:GrLicense ;
    linkml:is_a okns:GrBusinessFunction ;
    linkml:slot_usage [ ] ;
