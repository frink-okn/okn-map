okns:gr_eligibleCustomerTypes a okns:SlotDefinition ;
    dcterms:source <http://purl.org/goodrelations/v1> ;
    dcterms:title "eligible customer types (0..*)" ;
    skos:definition "The types of customers (gr:BusinessEntityType) for which the given gr:Offering is valid." ;
    skos:editorialNote "No occurrences of this slot in the graph." ;
    skos:inScheme okns:gr ;
    skos:mappingRelation <http://purl.org/goodrelations/v1#eligibleCustomerTypes> ;
