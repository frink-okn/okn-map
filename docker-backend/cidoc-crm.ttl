    linkml:definition_uri okns:Cidoc-crmE37Mark ;
    linkml:is_a okns:Cidoc-crmE36VisualItem ;
    linkml:slot_usage [ ] .

okns:cidoc_crm_P121_overlaps_with a okns:SlotDefinition ;
    dcterms:title "overlaps with" ;
    skos:definition "This symmetric property associates an instance of E53 Place with another instance of E53 Place geometrically overlapping it.␊It does not specify anything about the shared area. This property is purely spatial. It does not imply that phenomena that define, by their extent, places related by P121 overlaps with have ever covered a common area at the same time or even coexisted. In contrast, spatiotemporal overlaps described by P132 spatiotemporally overlaps are the total of areas simultaneously covered by the related spacetime volumes.␊This property is symmetric. This property is reflexive." ;
    skos:editorialNote "No occurrences of this slot in the graph." ;
    skos:inScheme okns:cidoc-crm ;
    skos:mappingRelation cidoc-crm:P121_overlaps_with ;
    skos:note "title: se superpose partiellement à",
        "title: sobrepõe com",
        "title: überlappt mit",
        "title: επικαλύπτεται με",
        "title: пeрeсeкаeтся с",
        "title: 重叠于" ;
    linkml:definition_uri okns:cidoc_crm_P121_overlaps_with ;
    linkml:domain okns:Cidoc-crmE53Place ;
    linkml:inlined true ;
    linkml:owner okns:cidoc_crm_P121_overlaps_with ;
    linkml:range okns:Cidoc-crmE53Place ;
    linkml:slot_uri cidoc-crm:P121_overlaps_with .

okns:cidoc_crm_P122_borders_with a okns:SlotDefinition ;
    dcterms:title "borders with" ;
    skos:definition "This symmetric property associates an instance of E53 Place with another instance of E53 Place which shares a part of its border.␊This property is purely spatial. It does not imply that the phenomena that define, by their extent, places related by P122 borders with have ever shared a respective border at the same time or even coexisted. In particular, this may be the case when the respective common border is formed by a natural feature.␊This property is not transitive. This property is symmetric." ;
    skos:editorialNote "No occurrences of this slot in the graph." ;
    skos:inScheme okns:cidoc-crm ;
    skos:mappingRelation cidoc-crm:P122_borders_with ;
