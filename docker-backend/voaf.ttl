okns:voaf_propertyNumber a okns:SlotDefinition ;
    dcterms:source <http://purl.org/vocommons/voaf> ;
    dcterms:title "number of properties" ;
    skos:definition "The number of properties defined in the vocabulary namespace. Properties imported from other namespaces are not taken into account." ;
    skos:editorialNote "No occurrences of this slot in the graph." ;
    skos:inScheme okns:voaf ;
    skos:mappingRelation voaf:propertyNumber ;
    skos:note "description: Le nombre de propriétés définies dans l'espace de noms du Vocabulaire. Les propriétés importées d'autres vocabulaires ne sont pas comptabilisées.",
        """description: PREFIX rdf:<http://www.w3.org/1999/02/22-rdf-syntax-ns#>
		PREFIX rdfs:<http://www.w3.org/2000/01/rdf-schema#>
		PREFIX owl:<http://www.w3.org/2002/07/owl#>
		PREFIX voaf:<http://purl.org/vocommons/voaf#>
		
		CONSTRUCT{
		?vocab voaf:propertyNumber ?nbProp
		}
		WHERE{
			SELECT (COUNT(distinct ?prop) AS ?nbProp) ?vocab
			WHERE{
				{?prop a rdf:Property.}
				UNION{?prop a owl:ObjectProperty.}
				UNION{?prop a owl:DatatypeProperty.}
				UNION{?prop a owl:AnnotationProperty.}
				UNION{?prop a owl:FunctionalProperty.}
				UNION{?prop a owl:OntologyProperty.}
				UNION{?prop a owl:AsymmetricProperty.}
				UNION{?prop a owl:InverseFunctionalProperty.}
				UNION{?prop a owl:IrreflexiveProperty.}
				UNION{?prop a owl:ReflexiveProperty.}
				UNION{?prop a owl:SymmetricProperty.}
				UNION{?prop a owl:TransitiveProperty.}
				?prop a ?type.
				FILTER(?type!=owl:DeprecatedProperty)
				?prop rdfs:isDefinedBy ?vocab.
				?vocab a voaf:Vocabulary.
			}GROUP BY ?vocab
		}""",
        "title: nombre de propriétés" ;
    linkml:definition_uri okns:voaf_propertyNumber ;
    linkml:domain okns:VoafVocabulary ;
    linkml:owner okns:voaf_propertyNumber ;
    linkml:range okns:integer ;
    linkml:slot_uri voaf:propertyNumber .

