okns:voaf_extends a okns:SlotDefinition ;
    dcterms:source <http://purl.org/vocommons/voaf> ;
    dcterms:title "extends" ;
    rdfs:subPropertyOf okns:voaf_reliesOn ;
    skos:definition "Indicates that the subject vocabulary extends the expressivity of the object vocabulary by declaring subsumption relationships, using object vocabulary class as domain or range of a subject vocabulary property, defining local restrictions etc ..." ;
    skos:editorialNote "No occurrences of this slot in the graph." ;
    skos:inScheme okns:voaf ;
    skos:mappingRelation voaf:extends ;
    skos:note "description: Indique que le vocabulaire sujet étend l'expressivité du vocabulaire objet en déclarant des relations de subsomption avec ce dernier, ou en utilisant ses classes comme domaine ou co-domaine d'un propriété du vocabulaire sujet, ou en définissant des contraintes locales etc.",
        """description: PREFIX rdfs:<http://www.w3.org/2000/01/rdf-schema#>
		PREFIX owl:<http://www.w3.org/2002/07/owl#>
		PREFIX voaf:<http://purl.org/vocommons/voaf#>

		CONSTRUCT{
			?vocab1 voaf:extends ?vocab2
		}
		WHERE{
			{?elem1 owl:inverseOf ?elem2. FILTER(!isBlank(?elem2))}
			UNION{?elem1 rdfs:domain ?elem2. FILTER(!isBlank(?elem2))}
			UNION{?elem1 rdfs:range ?elem2. FILTER(!isBlank(?elem2))}
			UNION{?elem2 rdfs:domain ?elem1. FILTER(!isBlank(?elem1))}
			UNION{?elem2 rdfs:range ?elem1. FILTER(!isBlank(?elem1))}
			?elem1 rdfs:isDefinedBy ?vocab1.
			?vocab1 a voaf:Vocabulary.
			?elem2 rdfs:isDefinedBy ?vocab2.
			?vocab2 a voaf:Vocabulary.
			FILTER(?vocab1!=?vocab2)
		}""",
        "title: étend" ;
    linkml:definition_uri okns:voaf_extends ;
    linkml:domain okns:VoafVocabulary ;
    linkml:inlined true ;
    linkml:owner okns:voaf_extends ;
    linkml:range okns:VoafVocabulary ;
    linkml:slot_uri voaf:extends .

okns:voaf_generalizes a okns:SlotDefinition ;
    dcterms:source <http://purl.org/vocommons/voaf> ;
    dcterms:title "generalizes" ;
    rdfs:subPropertyOf okns:voaf_reliesOn ;
    skos:definition "Indicates that the subject vocabulary generalizes by some superclasses or superproperties the object vocabulary." ;
    skos:editorialNote "No occurrences of this slot in the graph." ;
    skos:inScheme okns:voaf ;
    skos:mappingRelation voaf:generalizes ;
    skos:note "description: Indique que le vocabulaire sujet définit des super-classes ou super-propriétés du vocabulaire objet",
        """description: PREFIX rdf:<http://www.w3.org/1999/02/22-rdf-syntax-ns#>
		PREFIX rdfs:<http://www.w3.org/2000/01/rdf-schema#>
		PREFIX owl:<http://www.w3.org/2002/07/owl#>
		PREFIX voaf:<http://purl.org/vocommons/voaf#>
		PREFIX skos:<http://www.w3.org/2004/02/skos/core#>
		
		CONSTRUCT{
			?vocab1 voaf:generalizes ?vocab2
		}
		WHERE{
			{?elem1 skos:narrowMatch ?elem2. FILTER(!isBlank(?elem2))}
			UNION{?elem2 rdfs:subPropertyOf ?elem1.}
			UNION{?elem1 a owl:Class. ?elem1 owl:unionOf ?union. ?union rdf:first ?elem2.}
			UNION{?elem1 a owl:Class. ?elem1 owl:unionOf ?union. ?union rdf:rest ?union2. ?union2 rdf:first ?elem2.}
			UNION{?elem1 a owl:Class. ?elem1 owl:unionOf ?union. ?union rdf:rest ?union2. ?union2 rdf:rest ?union3. ?union3 rdf:first ?elem2.}
			UNION{?elem1 a owl:Class. ?elem1 owl:unionOf ?union. ?union rdf:rest ?union2. ?union2 rdf:rest ?union3. ?union3 rdf:rest ?union4. ?union4 rdf:first ?elem2.}
			UNION{?elem1 a owl:Class. ?elem1 owl:unionOf ?union. ?union rdf:rest ?union2. ?union2 rdf:rest ?union3. ?union3 rdf:rest ?union4. ?union4 rdf:rest ?union5. ?union5 rdf:first ?elem2.}
			FILTER(!isBlank(?elem2))
			?elem1 rdfs:isDefinedBy ?vocab1.
			?vocab1 a voaf:Vocabulary.
			?elem2 rdfs:isDefinedBy ?vocab2.
			?vocab2 a voaf:Vocabulary.
			FILTER(?vocab1!=?vocab2)
		}""",
        "title: généralise" ;
    linkml:definition_uri okns:voaf_generalizes ;
    linkml:domain okns:VoafVocabulary ;
    linkml:inlined true ;
    linkml:owner okns:voaf_generalizes ;
    linkml:range okns:VoafVocabulary ;
    linkml:slot_uri voaf:generalizes .

okns:voaf_hasDisjunctionsWith a okns:SlotDefinition ;
    dcterms:source <http://purl.org/vocommons/voaf> ;
    dcterms:title "has disjunctions with" ;
    rdfs:subPropertyOf okns:voaf_reliesOn ;
    skos:definition "Indicates that the subject vocabulary contains some declaration of disjoint classes with the object vocabulary." ;
    skos:editorialNote "No occurrences of this slot in the graph." ;
    skos:inScheme okns:voaf ;
    skos:mappingRelation voaf:hasDisjunctionsWith ;
    skos:note "description: Indique que le vocabulaire sujet contient des déclarations de classes disjointes avec le vocabulaire objet",
        """description: PREFIX rdfs:<http://www.w3.org/2000/01/rdf-schema#>
		PREFIX owl:<http://www.w3.org/2002/07/owl#>
		PREFIX voaf:<http://purl.org/vocommons/voaf#>
		
		CONSTRUCT{
			?vocab1 voaf:hasDisjunctionsWith ?vocab2
		}
		WHERE{
			?elem1 owl:disjointWith ?elem2. FILTER(!isBlank(?elem2))
			?elem1 rdfs:isDefinedBy ?vocab1.
			?vocab1 a voaf:Vocabulary.
			?elem2 rdfs:isDefinedBy ?vocab2.
			?vocab2 a voaf:Vocabulary.
			FILTER(?vocab1!=?vocab2)
		}""",
        "title: a des disjonctions avec" ;
    linkml:definition_uri okns:voaf_hasDisjunctionsWith ;
    linkml:domain okns:VoafVocabulary ;
    linkml:inlined true ;
    linkml:owner okns:voaf_hasDisjunctionsWith ;
    linkml:range okns:VoafVocabulary ;
    linkml:slot_uri voaf:hasDisjunctionsWith .

okns:voaf_hasEquivalencesWith a okns:SlotDefinition ;
    dcterms:source <http://purl.org/vocommons/voaf> ;
    dcterms:title "has equivalences with" ;
    rdfs:subPropertyOf okns:voaf_reliesOn ;
    skos:definition "Indicates that the subject vocabulary declares some equivalent classes or properties with the object vocabulary." ;
    skos:editorialNote "No occurrences of this slot in the graph." ;
    skos:inScheme okns:voaf ;
    skos:mappingRelation voaf:hasEquivalencesWith ;
    skos:note "description: Indique que le vocabulaire sujet déclare des équivalences de classes ou de propriétés avec le vocabulaire objet",
        """description: PREFIX rdfs:<http://www.w3.org/2000/01/rdf-schema#>
		PREFIX owl:<http://www.w3.org/2002/07/owl#>
		PREFIX voaf:<http://purl.org/vocommons/voaf#>
		PREFIX skos:<http://www.w3.org/2004/02/skos/core#>
		
		CONSTRUCT{
			?vocab1 voaf:hasEquivalencesWith ?vocab2
		}
		WHERE{
			{?elem1 owl:equivalentProperty ?elem2.}
			UNION{?elem1 owl:sameAs ?elem2.}
			UNION{?elem1 owl:equivalentClass ?elem2.}
			UNION{?elem2 owl:equivalentProperty ?elem1.}
			UNION{?elem2 owl:equivalentClass ?elem1.}
			UNION{?elem1 skos:exactMatch ?elem2.}
			UNION{?elem2 skos:exactMatch ?elem1.}
			FILTER(!isBlank(?elem2))
			?elem1 rdfs:isDefinedBy ?vocab1.
			?vocab1 a voaf:Vocabulary.
			?elem2 rdfs:isDefinedBy ?vocab2.
			?vocab2 a voaf:Vocabulary.
			FILTER(?vocab1!=?vocab2)
		}""",
        "title: a des équivalences avec" ;
    linkml:definition_uri okns:voaf_hasEquivalencesWith ;
    linkml:domain okns:VoafVocabulary ;
    linkml:inlined true ;
    linkml:owner okns:voaf_hasEquivalencesWith ;
    linkml:range okns:VoafVocabulary ;
    linkml:slot_uri voaf:hasEquivalencesWith .

