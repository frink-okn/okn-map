            linkml:tag okns:counts ],
        [ skos:example [ linkml:classes [ skos:example [ ] ;
                            linkml:tag okns:classes ] ;
                    linkml:pairs [ skos:example [ ] ;
                            linkml:tag okns:pairs ] ] ;
            linkml:tag okns:examples ] ;
