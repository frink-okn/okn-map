okns:AoLikeableAssociation a okns:ClassDefinition ;
    dcterms:source <http://purl.org/ontology/ao/core#> ;
    dcterms:title "Likeable Association" ;
    skos:definition "A likeable association. That means this association type is intended to be used for voting (ao:likeminded), feedbacking (rev:Feedback), reviewing (e.g. rev:rating) an association statement (related by ao:included_association) in the context of something." ;
    skos:inScheme okns:ao ;
    skos:mappingRelation <ao:LikeableAssociation> ;
    linkml:class_uri <http://purl.org/ontology/ao/core#LikeableAssociation> ;
    linkml:definition_uri okns:AoLikeableAssociation ;
    linkml:is_a okns:RevReview ;
    linkml:slot_usage [ ] .

