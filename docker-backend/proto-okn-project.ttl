@prefix dct: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix okn: <https://purl.org/okn/> .
@prefix okns: <https://purl.org/okn/schema/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

# Define the Proto-OKN Project
okn:proto-okn a foaf:Project ;
    dct:title "Prototype Open Knowledge Network" ;
    dct:description "The Prototype Open Knowledge Network (Proto-OKN) is a collaborative effort to develop interconnected knowledge graphs across multiple domains including biology, health, environment, justice, and manufacturing." ;
    rdfs:seeAlso <https://www.proto-okn.net/> .

# Link T1 graphs to the Proto-OKN project

# Biology and Health
okns:biobricks-ice dct:isPartOf okn:proto-okn .
okns:spoke dct:isPartOf okn:proto-okn .

# Environment
okns:climatemodelskg dct:isPartOf okn:proto-okn .
okns:fiokg dct:isPartOf okn:proto-okn .
okns:hydrologykg dct:isPartOf okn:proto-okn .
okns:sawgraph dct:isPartOf okn:proto-okn .
okns:sockg dct:isPartOf okn:proto-okn .
okns:spatialkg dct:isPartOf okn:proto-okn .
okns:ufokn dct:isPartOf okn:proto-okn .
okns:wildlifekn dct:isPartOf okn:proto-okn .

# Justice
okns:dreamkg dct:isPartOf okn:proto-okn .
okns:nikg dct:isPartOf okn:proto-okn .
okns:ruralkg dct:isPartOf okn:proto-okn .
okns:scales dct:isPartOf okn:proto-okn .

# Technology and Manufacturing
okns:securechainkg dct:isPartOf okn:proto-okn .
okns:sudokn dct:isPartOf okn:proto-okn .
