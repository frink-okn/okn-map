okns:SimInfluence a okns:ClassDefinition ;
    dcterms:source <http://purl.org/ontology/similarity/> ;
    dcterms:title "Influence" ;
    skos:definition "An abstract class indicating a directed association of influence where the subject entity has influenced the object entity." ;
    skos:inScheme okns:sim ;
    skos:mappingRelation <sim:Influence> ;
    linkml:class_uri <http://purl.org/ontology/similarity/Influence> ;
    linkml:definition_uri okns:SimInfluence ;
    linkml:is_a okns:SimAssociation ;
    linkml:slot_usage [ ] .

okns:SimSimilarity a okns:ClassDefinition ;
    dcterms:source <http://purl.org/ontology/similarity/> ;
    dcterms:title "Similarity" ;
    skos:definition "An abstract class to define similarity between two or more things.  Entities share a similarity if they share some common characteristics of interest.  A similarity is a special type of association." ;
    skos:inScheme okns:sim ;
    skos:mappingRelation <sim:Similarity> ;
    linkml:class_uri <http://purl.org/ontology/similarity/Similarity> ;
    linkml:definition_uri okns:SimSimilarity ;
    linkml:is_a okns:SimAssociation ;
    linkml:slot_usage [ ] .

okns:SimNetwork a okns:ClassDefinition ;
    dcterms:source <http://purl.org/ontology/similarity/> ;
    dcterms:title "Network" ;
    skos:definition "A network is a grouping of <code>sim:Association</code>s.  The associations that comprise a network are specified using a series of <code>sim:edge</code> predicates." ;
    skos:inScheme okns:sim ;
    skos:mappingRelation <sim:Network> ;
    linkml:class_uri <http://purl.org/ontology/similarity/Network> ;
    linkml:definition_uri okns:SimNetwork ;
    linkml:is_a okns:OwlThing ;
    linkml:slot_usage [ ] .

okns:sim_association a okns:SlotDefinition ;
    dcterms:source <http://purl.org/ontology/similarity/> ;
    dcterms:title "association" ;
    skos:definition "Binds a sim:Association to an arbitrary thing." ;
    skos:editorialNote "No occurrences of this slot in the graph." ;
    skos:inScheme okns:sim ;
    skos:mappingRelation <http://purl.org/ontology/similarity/association> ;
    linkml:definition_uri okns:sim_association ;
    linkml:domain okns:OwlThing ;
    linkml:inlined true ;
    linkml:owner okns:sim_association ;
    linkml:range okns:SimAssociation ;
    linkml:slot_uri <http://purl.org/ontology/similarity/association> .

okns:sim_description a okns:SlotDefinition ;
    dcterms:source <http://purl.org/ontology/similarity/> ;
    dcterms:title "description" ;
    skos:definition "Specifies some description that discloses the process or set of processes used to derive association statements for the given <code>AssociationMethod</code>.  This property is depricated in favor of the more appropriately named <code>sim:workflow</code> property." ;
    skos:editorialNote "No occurrences of this slot in the graph." ;
    skos:exactMatch <http://purl.org/ontology/similarity/workflow> ;
