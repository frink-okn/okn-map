okns:sim_weight a okns:SlotDefinition ;
    dcterms:source <http://purl.org/ontology/similarity/> ;
    dcterms:title "weight" ;
    skos:definition "A weighting value bound to a <code>sim:Association</code> where a value of 0 implies two elements are not at all associated and a higher value implies a closer association." ;
    skos:editorialNote "No occurrences of this slot in the graph." ;
    skos:inScheme okns:sim ;
    skos:mappingRelation <http://purl.org/ontology/similarity/weight> ;
