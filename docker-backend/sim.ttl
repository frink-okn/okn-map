    skos:mappingRelation <http://purl.org/ontology/similarity/description> ;
    linkml:definition_uri okns:sim_description ;
    linkml:domain okns:SimAssociationMethod ;
    linkml:inlined true ;
    linkml:owner okns:sim_description ;
    linkml:range okns:Any ;
    linkml:slot_uri <http://purl.org/ontology/similarity/description> .

okns:sim_distance a okns:SlotDefinition ;
    dcterms:source <http://purl.org/ontology/similarity/> ;
    dcterms:title "distance" ;
    skos:definition "A weighting value for an Association where a value of 0 implies two elements are the same individual." ;
    skos:editorialNote "No occurrences of this slot in the graph." ;
