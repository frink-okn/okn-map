    dcterms:source <http://purl.org/ontology/similarity/> ;
    dcterms:title "method" ;
    skos:definition "Specifies the <code>sim:AssociationMethod</code> used to derive a particular Association statement.  This should be used when the process for deriving association statements can be described further." ;
    skos:editorialNote "No occurrences of this slot in the graph." ;
